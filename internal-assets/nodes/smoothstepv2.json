{
    "name": "SmoothstepV2",
    "inputs": [
        {
            "type": "vec2",
            "name": "Edge0"
        },
        {
            "type": "vec2",
            "name": "Edge1"
        },
        {
            "type": "float",
            "name": "In"
        }
    ],
    "outputs": [
        {
            "type": "vec2",
            "name": "Result"
        }
    ],
    "glsl": " Result = vec2(smoothstep(Edge0.x, Edge1.x, In), smoothstep(Edge0.y, Edge1.y, In)); "
}